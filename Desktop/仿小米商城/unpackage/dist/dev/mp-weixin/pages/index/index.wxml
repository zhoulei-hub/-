<view class="uni-tab-bar"><scroll-view class="uni-swiper-tab" id="tab-bar" scroll-x="{{true}}" scroll-left="{{scrollLeft}}"><block wx:for="{{tabBars}}" wx:for-item="tab" wx:for-index="index" wx:key="id"><view class="{{['swiper-tab-list',tabIndex==index?'active':'']}}" id="{{tab.id}}" data-current="{{index}}" data-event-opts="{{[['tap',[['tapTab',['$event']]]]]}}" bindtap="__e">{{tab.name}}</view></block></scroll-view><swiper class="swiper-box" current="{{tabIndex}}" duration="{{300}}" data-event-opts="{{[['change',[['changeTab',['$event']]]]]}}" bindchange="__e"><block wx:for="{{newsitems}}" wx:for-item="tab" wx:for-index="index1" wx:key="index1"><swiper-item><scroll-view class="list" scroll-y="{{true}}" data-event-opts="{{[['scrolltolower',[['loadMore',[index1]]]]]}}" bindscrolltolower="__e"><block wx:if="{{tabBars[index1].template==='index'}}"><swiper-image vue-id="{{'8dd740cc-1-'+index1}}" banners="{{tab.data.banners}}" bind:__l="__l"></swiper-image><index-nav vue-id="{{'8dd740cc-2-'+index1}}" indexnav="{{tab.data.indexNav}}" bind:__l="__l"></index-nav><block wx:for="{{tab.data.list}}" wx:for-item="v" wx:for-index="i" wx:key="i"><block><block wx:if="{{v.type==='ThreeAdverts'}}"><divider vue-id="{{'8dd740cc-3-'+index1+'-'+i}}" bind:__l="__l"></divider><three-adverts vue-id="{{'8dd740cc-4-'+index1+'-'+i}}" resdata="{{v.data}}" bind:__l="__l"></three-adverts></block><block wx:if="{{v.type==='OneAdvert'}}"><divider vue-id="{{'8dd740cc-5-'+index1+'-'+i}}" bind:__l="__l"></divider><card vue-id="{{'8dd740cc-6-'+index1+'-'+i}}" title="{{v.data.name}}" cover="{{v.data.src}}" bind:__l="__l"></card></block><block wx:if="{{v.type==='list'}}"><view class="row" style="margin:0 -5rpx;"><block wx:for="{{v.data}}" wx:for-item="item" wx:for-index="idx" wx:key="idx"><block><common-list vue-id="{{'8dd740cc-7-'+index1+'-'+i+'-'+idx}}" item="{{item}}" bind:__l="__l"></common-list></block></block></view></block></block></block></block><block wx:if="{{tabBars[index1].template==='special'}}"><swiper-image vue-id="{{'8dd740cc-8-'+index1}}" banners="{{tab.data.banners}}" bind:__l="__l"></swiper-image><index-nav vue-id="{{'8dd740cc-9-'+index1}}" indexnav="{{tab.data.indexNav}}" bind:__l="__l"></index-nav><block wx:for="{{tab.data.list}}" wx:for-item="v" wx:for-index="i" wx:key="i"><block><block wx:if="{{v.type==='list'}}"><view class="row" style="margin:0 -5rpx;"><block wx:for="{{v.data}}" wx:for-item="item" wx:for-index="idx" wx:key="idx"><block><common-list vue-id="{{'8dd740cc-10-'+index1+'-'+i+'-'+idx}}" item="{{item}}" bind:__l="__l"></common-list></block></block></view></block></block></block></block><view class="uni-tab-bar-loading">{{''+tab.loadingText+''}}</view></scroll-view></swiper-item></block></swiper></view>